{
  "manifest_version": 1,
  "version": 1,
  "type": "APP",
  "name": {
    "en": "Export kintone data to Google Spreadsheet",
    "ja": "Googleスプレッドシートにデータ出力"
  },
  "description": {
    "en": "⚠ This plugin supports only Japanese",
    "ja": "kintone のレコードデータを Google スプレッドシートへ出力します"
  },
  "icon": "image/icon.png",
  "homepage_url": {
    "en": "https://github.com/senseiplace/kintone-export-to-google-spreadsheet",
    "ja": "https://github.com/senseiplace/kintone-export-to-google-spreadsheet"
  },
  "desktop": {
    "js": [
      "js/desktop.js"
    ],
    "css": [
      "css/desktop.css"
    ]
  },
  "config": {
    "html": "html/config.html",
    "js": [
      "js/config.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/config.css"
    ],
    "required_params": [
      "spreadsheetId",
      "serviceAccountClientEmail",
      "serviceAccountPrivateKey"
    ]
  }
}
